{
  "name": "bumpwin",
  "version": "0.202505.4",
  "description": "SDK for BUMP.WIN",
  "author": "BUMP.WIN",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fuyutarow/bumpwin-pilot.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "prepare": "bunx tsup",
    "suigen:devnet": "rm -rf src/suigen && sui-client-gen -m ../suigen-configs/devnet.toml -o src/suigen && bun format",
    "suigen:testnet": "rm -rf src/suigen && sui-client-gen -m ../suigen-configs/testnet.toml -o src/suigen && bun format",
    "lint": "bunx @biomejs/biome check --fix .",
    "format": "bunx @biomejs/biome format --write ."
  },
  "dependencies": {
    "@mysten/sui": "^1.29.1",
    "@mysten/walrus": "^0.0.21"
  },
  "devDependencies": {
    "@biomejs/biome": "latest",
    "@types/bun": "latest",
    "@types/node": "^22.15.3",
    "tsup": "^8.4.0",
    "vitest": "^3.1.2"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "keywords": [
    "sui",
    "sdk",
    "typescript"
  ]
}
