{
  "name": "oozefun-sdk",
  "version": "0.0.1",
  "description": "SDK for OozeFun",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "prepare": "tsup",
    "lint": "bunx @biomejs/biome check --fix .",
    "format": "bunx @biomejs/biome format --write ."
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.4.0",
    "vitest": "^3.1.2",
    "@biomejs/biome": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "keywords": [
    "sui",
    "sdk",
    "typescript"
  ],
  "author": "ooze.fu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ooze-pilot.git"
  }
}