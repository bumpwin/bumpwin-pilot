{
  "name": "bumpwin",
  "version": "0.202505.21",
  "description": "SDK for BUMP.WIN",
  "author": "BUMP.WIN",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fuyutarow/bumpwin-pilot.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./suigen": {
      "types": "./dist/suigen/index.d.ts",
      "import": "./dist/esm/suigen/index.js",
      "require": "./dist/cjs/suigen/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "prepare": "bun run build",
    "prepublishOnly": "npm run build",
    "suigen:devnet": "rm -rf src/suigen && suigen -m ../suigen-configs/devnet.toml -o src/suigen && bun format",
    "suigen:testnet": "rm -rf src/suigen && suigen -m ../suigen-configs/testnet.toml -o src/suigen && bun format",
    "lint": "biome check --fix .",
    "format": "biome format --write .",
    "fmt": "bun run format && bun run lint"
  },
  "dependencies": {
    "@mysten/bcs": "^1.6.1",
    "@mysten/seal": "^0.4.4",
    "@mysten/sui": "^1.30.0",
    "@mysten/walrus": "^0.0.21",
    "neverthrow": "^8.2.0",
    "pretty-bytes": "^7.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@types/bun": "^1.1.0",
    "@types/node": "^22.15.21",
    "tsup": "^8.5.0",
    "vitest": "^3.1.4"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "keywords": [
    "sui",
    "sdk",
    "typescript"
  ]
}
