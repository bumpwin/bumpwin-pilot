{
  "$schema": "https://biomejs.dev/schemas/1.5.3/schema.json",
  "organizeImports": {
    "enabled": true
  },
  "linter": {
    "enabled": true,
    "ignore": [
      "**/dist/**",
      "**/moveCall/**"
    ]
  },
  "formatter": {
    "enabled": true,
    "ignore": [
      "**/dist/**",
      "**/moveCall/**"
    ]
  },
  "javascript": {
    "formatter": {
      "quoteStyle": "single",
      "trailingComma": "es5",
      "semicolons": "always"
    }
  }
}